{
  "expo": {
    "name": "W7 Animations",
    "slug": "w7-animations",
    "description": "W7 Animations",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "BatmanCodes.W7Animations",
      "buildNumber": "4",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.BatmanCodes.W7Animations",
      "permissions": [
        "CAMERA_ROLL",
        "CAMERA",
        "NOTIFICATIONS"
      ],
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
